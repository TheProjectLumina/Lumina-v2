{
  "namespace": "zxyn_hud",
  /*Code Di buat Oleh Han's

  Beberapa fitur yang bukan buatan Han's:
  1. Durabilty Ping dari @TheAndromedarius.
  2. Item Percentage dari @DVG MCPE YT

  Dilarang menggunakan code apapun dari NekoUI tanpa izin dari Han's. Dan 
  Dilarang menggunakan code dari beberapa fitur di atas tanpa izin dari Creator yang membuat fitur tersebut

  Han's sudah meminta izin kepada TheAndromedarius dan DVG MCPE YT untuk menggunakan fitur tsb.
  //(c) Don't use any code design without permission!*/

  "dura_srart_offset": {
    "anim_type": "offset",
    "from": ["150%", -25],
    "to": [0, -25],
    "duration": 0.8
  },
  "dura_wait_offset": {
    "anim_type": "wait",
    "duration": 3,
    "next": "@hud.dura_end_offset"
  },
  "dura_end_offset": {
    "anim_type": "offset",
    "from": [0, -25],
    "to": ["150%", -25],
    "duration": 0.8
  },
  "dura_srart_alpha": {
    "anim_type": "wait",
    "duration": 3,
    "next": "@hud.dura_end_alpha"
  },
  "dura_end_alpha": {
    "anim_type": "alpha",
    "from": 1,
    "to": 0,
    "duration": 1.6
  },

  /*//// MAIN PANEL ////*/
  "durability_ping_panel": {
    "type": "panel",
    "offset": [ 31 , 0 ],
    "anchor_from": "bottom_right",
    "anchor_to": "bottom_right",
    "size": ["180px", "36px"],
    "controls": [{ "durability_ping_grid@zxyn_hud.durability_ping_grid": {} }]
  },

  // GRID
  "durability_ping_grid": {
    "type": "grid",
    "grid_item_template": "zxyn_hud.durability_ping_control_panel",
    "grid_dimension_binding": "#hotbar_grid_dimensions",
    "collection_name": "hotbar_items",
    "size": ["100%cm", "100%cm"],
    "bindings": [
      { "binding_name": "#hotbar_grid_dimensions", "binding_type": "global" }
    ],
    "property_bag": { "force_update": true }
  },

  // MAIN HAND SLOT SELECTION
  "durability_ping_control_panel": {
    "type": "panel",
    "controls": [{ "durability_ping_bd_2@zxyn_hud.durability_ping_bd_2": {} }],
    "bindings": [
      {
        "binding_name": "(not (#item_durability_total_amount = 0))",
        "binding_name_override": "#visible",
        "binding_type": "collection",
        "binding_collection_name": "hotbar_items"
      }
    ]
  },

  // ITEM SELECTION (NOT HAND)
  "durability_ping_bd_2": {
    "type": "stack_panel",
    "size": ["110px + 1%", "30px + 1%"],
    "factory": { "control_name": "zxyn_hud.durability_ping_bg" },
    "$min_durability": 0,
    "$max_durability": 0.26,
    "collection_name": "durability_ping",
    "bindings": [
      {
        "binding_name": "#item_durability_total_amount",
        "binding_type": "collection",
        "binding_collection_name": "hotbar_items"
      },
      {
        "binding_name": "#item_durability_current_amount",
        "binding_type": "collection",
        "binding_collection_name": "hotbar_items"
      },
      {
        "binding_name": "#slot_selected",
        "binding_type": "collection",
        "binding_collection_name": "hotbar_items"
      },
      {
        "binding_type": "view",
        "source_property_name": "((((#item_durability_current_amount / #item_durability_total_amount) > $min_durability) and ((#item_durability_current_amount / #item_durability_total_amount) < $max_durability)) or ((#item_durability_current_amount / #item_durability_total_amount) = $min_durability) and #slot_selected)",
        "target_property_name": "#colst"
      },
      {
        "binding_type": "view",
        "source_property_name": "(#colst * 1)",
        "target_property_name": "#collection_length"
      }
    ]
  },

  // STACK PANEL
  "durability_ping_bg": {
    "type": "panel",
    "controls": [
      {
        "durability_ping_bg_content@neko_c.bg_2": {
          "alpha": 0.55,
          "type": "image",
          "layer": -10,
          "size": ["100%", "100%"],
          "anims": ["@zxyn_hud.dura_srart_offset", "@zxyn_hud.dura_wait_offset", "@zxyn_hud.dura_srart_alpha"],
          "controls": [
            {
              "durability_ping_content": {
                "type": "stack_panel",
                "orientation": "horizontal",
                "size": [ "100%", "100%" ],
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "offset": [0, "0%"],
                "controls": [
                  { "padding_0": { "type": "panel", "size": [5, 0] } },
                  {
                    "durability_ping_item_renderer@zxyn_hud.durability_ping_item_renderer": {}
                  },
                  { "padding_1": { "type": "panel", "size": [5, 0] } },
                  { "durability_ping_alert@zxyn_hud.durability_ping_alert": {} }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  "neko_label": {
    "type": "label",
    "size": [ "100%", "default" ]
  },

  // TEXT & AUX
  "durability_ping_alert": {
    "type": "panel",
    "size": [ "fill", "100%" ],
    "controls": [
      {
        "stack": {
          "type": "stack_panel",
          "anchor_from": "center",
          "anchor_to": "center",
          "size": [ "100%", "100%c" ],
          "controls": [
            {
              "label_header@zxyn_hud.neko_label": {
              "text": "Â§fTool Durability Low!",
              "layer": 5,
              "font_type": "EudoxusSans-Bold",
              "font_scale_factor": 0.75
              }
            },
            {
              "zxyn_durability@zxyn_hud_content.zxyn_durability_percent_panel":{ "$neko_dura_font": "EudoxusSans-Bold"}
            }
          ]
        }
      }
    ]
  },

  // ITEM RENDEER
  "durability_ping_item_renderer": {
    "type": "stack_panel",
    "offset": [0, "50%"],
    "size": [22, "100%c"],
    "controls": [
      { "padding_0": { "type": "panel", "size": [0, 5] } },
      {
        "item_renderer@common.item_renderer": {
          "visible": false,
          "size": [22, 22],
          "$item_collection_name": "hotbar_items",
          "item_renderer_binding_condition": "always_when_visible"
        }
      }
    ]
  },

  /*//// SOUND NOTIF (MOBILE ONLY) ////*/
  "gui_hotbar_grid_item/hotbar_parent": {
    "modifications": {
      "array_name": "controls",
      "operation": "insert_front",
      "value": {
        "durability_ping_notification@durability_ping_notification": {}
      }
    }
  },
  "durability_ping_notification": {
    "type": "panel",
    "controls": [
      {
        "durability_ping_notification_content@zxyn_hud.durability_ping_notification_content": {}
      }
    ],
    "bindings": [
      {
        "binding_name": "(not (#item_durability_total_amount = 0))",
        "binding_name_override": "#visible",
        "binding_type": "collection",
        "binding_collection_name": "hotbar_items"
      }
    ]
  },
  "durability_ping_notification_content@zxyn_hud.gui_hotbar_button_prototype": {
    "layer": 3,
    "sound_name": "andro.durability_ping_notification",
    "$total_amount": "#item_durability_total_amount",
    "$current_amount": "#item_durability_current_amount",
    "$min_durability": 0.0,
    "$max_durability": 0.26,
    "controls": [],
    "bindings": [
      {
        "binding_collection_name": "$hotbar_collection_name",
        "binding_type": "collection_details"
      },
      {
        "binding_name": "$total_amount",
        "binding_type": "collection",
        "binding_collection_name": "hotbar_items"
      },
      {
        "binding_name": "$current_amount",
        "binding_type": "collection",
        "binding_collection_name": "hotbar_items"
      },
      {
        "binding_type": "view",
        "source_property_name": "((($current_amount / $total_amount) > $min_durability) and (($current_amount / $total_amount) < $max_durability)) or (($current_amount / $total_amount) = $min_durability)",
        "target_property_name": "#visible"
      }
    ]
  },

  "kordinat_player@neko_c.bg_2": {
    "alpha": 0.5,
    "anchor_from": "top_right",
    "anchor_to": "top_right",
    "offset": [ -4, 30 ],
    "size": [ "100%c + 10px", 15 ],
    "bindings": [
      {
        "binding_name": "#player_position_visible",
        "binding_name_override": "#visible",
        "binding_type": "global"
      }
    ],
    "controls": [
      {
        "kordinat_player": {
         "type": "panel",
          "alpha": 0.5,
          "size": [ "100%c", 10 ],
          "controls": [
            {
              "player_position_text": {
                "type": "label",
                "font_type": "EudoxusSans-Bold",
                "font_scale_factor": 0.75,
                "text": "#text",
                "bindings": [
                  {
                    "binding_name": "#player_position_text",
                    "binding_name_override": "#text",
                    "binding_condition": "always_when_visible",
                    "binding_type": "global"
                  }
                ]
              }
            }
          ]
        }      
      }
    ]
  },
  /// A

  "render_a": {"type": "custom","size": [ 17 , 17 ],"renderer": "inventory_item_renderer","property_bag": {"#item_id_aux": 22740992}},
  "render_b@render_a": {"property_bag": {"#item_id_aux": 22609920}},
  "render_c@render_a": {"property_bag": {"#item_id_aux": 42795008}},

