	"zxyn_durability_percent":{
		"$zxyn_total_bar":"#item_durability_total_amount",
		"$zxyn_current_bar":"#item_durability_current_amount",
		"bindings":[
			{
				"binding_name":"$zxyn_total_bar",
				"binding_type":"collection",
				"binding_collection_name":"hotbar_items"
			},
			{
				"binding_name":"$zxyn_current_bar",
				"binding_type":"collection",
				"binding_collection_name":"hotbar_items"
			},
			{
				"binding_type":"view",
				"source_property_name":"((($zxyn_current_bar / $zxyn_total_bar) > $min_durability) and (($zxyn_current_bar / $zxyn_total_bar) < $max_durability)) or (($zxyn_current_bar / $zxyn_total_bar) = $min_durability)",
				"target_property_name":"#visible"
			}
		]
	},
