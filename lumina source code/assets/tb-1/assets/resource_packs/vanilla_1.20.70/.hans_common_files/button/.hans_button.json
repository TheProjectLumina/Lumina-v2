        }
      },
      {
        "title": {
          "layer": 11,
          "size": "$neko_font_size",
          "type": "label",
          "visible": "$pressed_alpha",
          "font_type": "EudoxusSans-Bold",
          "font_scale_factor": "$neko_font_title",
          "alpha": "$label_use_animate",
          "anchor_from": "$label_anchor",
          "anchor_to": "$label_anchor",
          "color": "black",
          "text_alignment": "$neko_title_and_desc_text_alignment",
          "offset": "$neko_label_offset_title",
          "text": "$neko_label_title_text"
        }
      },
      {
        "desc": {
          "layer": 11,
          "ignored": "$do_not_use_desc",
          "size": "$neko_font_size",
          "type": "label",
          "visible": "$pressed_alpha",
          "font_type": "EudoxusSans-Bold",
          "font_scale_factor": "$neko_desc_title",
          "anchor_from": "$label_anchor",
          "anchor_to": "$label_anchor",
          "color": "black",
          "text_alignment": "$neko_title_and_desc_text_alignment",
          "offset": "$neko_label_offset_desc_text",
          "text": "$neko_label_desc_text"
        } 
      }
    ]
  },



