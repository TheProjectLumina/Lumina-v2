  },


  "han_uwu": {
    "type": "panel",
    "$ignored_realms_notif|default": true,
    "$neko_icon_alpha": 1,

    // Icon
    "$do_not_use_icon|default": false,
    "$icon_anchor|default": "left_middle",
    "$icon_size|default": [ 21, 21 ],
    "$icon_texture|default": "",
    "$icon_offset|default": [ 4.5, 0 ],

    "$label_use_animate|default": "",

    // Label on panel
    "$label_anchor|default": "left_middle",
    "$do_not_use_desc|default": false,
    "$neko_font_size|default": [ "100% - 35px", "default" ],
    "$neko_font_title|default": 1.6,
    "$neko_desc_title|default": 0.5,

    "$neko_label_title_text|default": "",
    "$neko_label_offset_title|default": [ 32 , -6.1 ],

    "$neko_label_desc_text|default": "",
    "$neko_label_offset_desc_text|default": [ 32 , 6.85 ],

    "$neko_title_and_desc_text_alignment|default": "left",
    
    "layer": 5,
    "controls": [
      {
        "a": {
          "type": "panel",
          "ignored": "$ignored_realms_notif",
          "controls": [
            {
              "count@start.notification_button_text_panel": {
                "offset": [ -2.5, 2 ]
              }
            }
          ],
          "bindings": [
            {
              "binding_name": "#realms_notification_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "glyph": {
          "type": "image",
          "layer": 15,
          "ignored": "$do_not_use_icon",
          "anchor_from": "$icon_anchor",
          "anchor_to": "$icon_anchor",
          "offset": "$icon_offset",
          "texture": "$icon_texture",
          "size": "$icon_size",
          "bilinear": true,
          "color": "$warna_icon",
          "alpha": "$neko_icon_alpha",
          "variables": [
            {
              "requires": "$default_state",
              "$glyph_offset": [ 2 , 0.5 ],
              "$warna_icon": "$main_ui_icon_color_default"
            },
            {
              "requires": "$hover_state",
              "$glyph_offset": [ 2 , 0.5 ],
              "$warna_icon": "$main_ui_icon_color_hover"
            },
            {
              "requires": "$pressed_state",
              "$glyph_offset": [ 2 , 0.5 ],
              "$warna_icon": "$main_ui_icon_color_pressed"
            },
            {
              "requires": "$locked_state",
              "$glyph_offset":[ 2 , 0.5 ],
              "$warna_icon": "$main_ui_icon_color_locked"
            }
          ],
          "$glyph_bindings|default": [],
          "bindings": "$glyph_bindings"
        }
      },
      {
        "title": {
          "layer": 11,
          "size": "$neko_font_size",
          "type": "label",
          "font_type": "EudoxusSans-Bold",
          "font_scale_factor": "$neko_font_title",
          "alpha": "$label_use_animate",
          "anchor_from": "$label_anchor",
          "anchor_to": "$label_anchor",
          "color": "$text_color",
          "text_alignment": "$neko_title_and_desc_text_alignment",
          "offset": "$neko_label_offset_title",
          "text": "$neko_label_title_text"
        }
      },
      {
        "desc": {
          "layer": 11,
          "ignored": "$do_not_use_desc",
          "size": "$neko_font_size",
          "type": "label",
          "font_type": "EudoxusSans-Bold",
          "font_scale_factor": "$neko_desc_title",
          "anchor_from": "$label_anchor",
          "anchor_to": "$label_anchor",
          "color": "$text_color",
          "text_alignment": "$neko_title_and_desc_text_alignment",
          "offset": "$neko_label_offset_desc_text",
          "text": "$neko_label_desc_text"
        } 
      }
    ]
  },

