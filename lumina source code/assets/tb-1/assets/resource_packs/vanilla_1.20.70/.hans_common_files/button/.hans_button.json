                "type": "panel",
                "size": [ "5px", "100%" ],
                "controls": [
                  {
                    "ico": {
                    }
                  }
                ]
              }
            },
            {
              "label": {
                "type": "panel",
                "size": [ "fill", "100%" ],
                "controls": [
                  {
                    "title": {
                      "layer": 11,
                      "size": [ "100%", "default" ],
                      "type": "label",
                      "anchor_from": "center",
                      "anchor_to": "center",
                      "color": "$text_color",
                      "text_alignment": "left",
                      "offset": "$neko_label_offset_title",
